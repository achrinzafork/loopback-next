{
  "name": "@loopback/rest-raml",
  "version": "0.1.0",
  "description": "A lightweight extension to convert the generated OpenAPI specifications into RAML 1.0 and expose it via a binding and optionally via the REST server.",
  "keywords": [
    "loopback-extension",
    "loopback",
    "RAML"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "^10.16 || 12 || 14 || 15"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "extensions/rest-raml"
  },
  "author": "IBM Corp.",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "copyright.owner": "IBM Corp.",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@loopback/core": "^2.14.1",
    "@loopback/rest": "^9.1.3"
  },
  "dependencies": {
    "tslib": "^2.0.0",
    "webapi-parser": "^0.5.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.9",
    "@loopback/core": "^2.14.1",
    "@loopback/rest": "^9.1.3",
    "@loopback/testlab": "^3.2.12",
    "@types/node": "^10.17.48",
    "typescript": "~4.1.3"
  }
}
